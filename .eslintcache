rs();\n        this.expiryTime = this.uploadTime.setHours(this.uploadTime + 24);\n    }\n\n\n    return Story;\n\n}\n\nexport async function useGetStories() {\n    return [\n        {\n            owner: {\n                firstName: \"Karan\",\n                lastName: \"pal\"\n            },\n            uploadTime: 0 + \"hrs\",\n            expiryTime: 24 + \"hrs\"\n        }\n\n    ]\n}\n\n","/home/celvin/Desktop/way2way-react/src/components/Story/Story.js",["85","86","87","88","89","90","91"],"import React, { useState } from 'react';\nimport { useCreateStory } from '../../assets/StoryHooks';\nimport './Stories.scss';\nimport FullScreenStory from '../FullscreenStory/FullScreenStory'\nfunction Story(props) {\n\n    // use username instead of firstname and lastname it would be much more easier\n    const {\n        firstName,\n        lastname,\n        image,\n        uploadTime,\n        expiryTime\n\n    } = props;\n\n    const [fullScreen, setshowfullScreen] = useState(false)\n\n\n    return (\n        <div className=\"story\">\n            <FullScreenStory\n                fullScreen={fullScreen}\n                setshowfullScreen={setshowfullScreen}\n                image={\"https://daily.jstor.org/wp-content/uploads/2019/05/the_quantum_random_number_generator_alt2_1050x700.jpg\"}\n            />\n            <div className=\"story-circle\" onClick={() => { setshowfullScreen(!fullScreen); console.log(\"clicked\") }}>\n                <img src=\"https://daily.jstor.org/wp-content/uploads/2019/05/the_quantum_random_number_generator_alt2_1050x700.jpg\" />\n            </div>\n        </div>\n    )\n}\nexport default Story;","/home/celvin/Desktop/way2way-react/src/components/FullscreenStory/FullScreenStory.js",["92","93","94"],"import React, { useEffect } from 'react';\nimport './FullScreenStory.scss';\nimport gsap from 'gsap';\n\nfunction FullScreenStory({ fullScreen, setshowfullScreen, image }) {\n\n    const goFullScreenAnimation = gsap.fromTo(\"#fullscreen-